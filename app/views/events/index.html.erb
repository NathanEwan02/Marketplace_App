<h1>Events</h1>

<p><%= link_to "Create New Event", new_event_path %></p>

<table>
    <tr>
        <th>Event Name</th>
        <th>Location</th>
        <th>Date</th>
    </tr>
    <% @events.each do |event| %>
        <tr>
            <td><%= link_to event.name, event %></td>
            <td><%= event.location %></td>
            <td><%= event.date %></td>
        </tr>
    <% end %>
</table>