<h1>Events</h1>

<table>
    <tr>
        <th>Event Name</th>
        <th>Location</th>
        <th>Date</th>
    </tr>
    <% @events.each do |event| %>
        <tr>
            <td><%= event.name %></td>
            <td><%= event.location %></td>
            <td><%= event.date %></td>
        </tr>
    <% end %>
</table>